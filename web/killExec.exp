#!/usr/bin/expect -f

#log user 0
set prompt "(%|#|\\\$) $"
catch {set prompt $env(EXPECT_PROMPT)}

set ip [lindex $argv 0]
set executable [lindex $argv 1]

spawn ./killExec.sh $ip $executable
expect {
"(yes/no)?" {send "yes\r"
               expect "password:"
               send "armored\r"
              }
  "password:" { send "armored\r"}
}

expect -re $prompt
